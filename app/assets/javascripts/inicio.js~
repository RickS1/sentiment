

$(document).ready(function(){
	setInterval(function(){
		$.ajax({
			url: '/inicio/obtener_stats',
			data: quote,
			type: 'GET',
			cache: false,
			success: function(data) {
				$("#resultados").html("Tweets actuales: " + data.tweets);
			},
			error: function() {
				$("#resultados").html("Error al recoger los datos.");
			}
		}), 
	60000);
});
